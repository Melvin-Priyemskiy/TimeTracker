<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <Button
        android:id="@+id/buttonEmployerLoginHome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="24dp"
        android:text="Employer Login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2EmployerCreateAcc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Employer Create Account"
        app:layout_constraintEnd_toEndOf="@+id/buttonEmployerLoginHome"
        app:layout_constraintStart_toStartOf="@+id/buttonEmployerLoginHome"
        app:layout_constraintTop_toBottomOf="@+id/buttonEmployerLoginHome" />

    <Button
        android:id="@+id/buttonEmployeeLoginHome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Employee Login"
        app:layout_constraintEnd_toEndOf="@+id/button2EmployerCreateAcc"
        app:layout_constraintStart_toStartOf="@+id/button2EmployerCreateAcc"
        app:layout_constraintTop_toBottomOf="@+id/button2EmployerCreateAcc" />
</androidx.constraintlayout.widget.ConstraintLayout>